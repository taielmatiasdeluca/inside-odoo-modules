<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <!-- MAIN TREE VIEW  -->
    <record id="view_stages_custom_tree" model="ir.ui.view">
            <field name="name">Stages Custom Tree View</field>
            <field name="model">stages.payment</field>
            <field name="arch" type="xml">            
                <tree string="Lista de Elementos"  create="false">
                    <field name="titulo"/>
                    <field name="descripcion"/>
                    <field name="precio"/>
                    <field name="stages"/>
                </tree>
            </field>
    </record>

  </data>
</odoo>
